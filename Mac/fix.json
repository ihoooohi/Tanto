{
    "description": "AHK Vim Mode (Final: Fixed ESC & Scroll)",
    "manipulators": [
        {
            "description": "1. [Unlock] If Locked: Tap CapsLock to Unlock (Deselect & Reset)",
            "type": "basic",
            "conditions": [ { "type": "variable_if", "name": "vim_sticky", "value": 1 } ],
            "from": { "key_code": "caps_lock", "modifiers": { "optional": ["any"] } },
            "to_if_alone": [
                { "shell_command": "osascript -e 'display notification \"üèÅ Mode Off\" with title \"Vim Mode\"'" },
                { "key_code": "right_arrow", "conditions": [ { "type": "variable_if", "name": "visual_mode", "value": 1 } ] },
                { "set_variable": { "name": "vim_sticky", "value": 0 } },
                { "set_variable": { "name": "vim_mode", "value": 0 } },
                { "set_variable": { "name": "visual_mode", "value": 0 } },
                { "set_variable": { "name": "delete_pending", "value": 0 } }
            ]
        },
        {
            "description": "2. [Activate] Unlocked Logic: Hold=Normal, Tap=Visual+Lock",
            "type": "basic",
            "conditions": [ { "type": "variable_if", "name": "vim_sticky", "value": 0 } ],
            "from": { "key_code": "caps_lock", "modifiers": { "optional": ["any"] } },
            "to": [ { "set_variable": { "name": "vim_mode", "value": 1 } } ],
            "to_if_alone": [
                { "set_variable": { "name": "vim_sticky", "value": 1 } },
                { "set_variable": { "name": "visual_mode", "value": 1 } },
                { "shell_command": "osascript -e 'display notification \"üëÅÔ∏è Visual Mode\" with title \"Vim Mode\"'" }
            ],
            "to_after_key_up": [
                { "key_code": "right_arrow", "conditions": [ { "type": "variable_if", "name": "visual_mode", "value": 1 }, { "type": "variable_if", "name": "vim_sticky", "value": 0 } ] },
                { "set_variable": { "name": "vim_mode", "value": 0 }, "conditions": [ { "type": "variable_if", "name": "vim_sticky", "value": 0 } ] },
                { "set_variable": { "name": "visual_mode", "value": 0 }, "conditions": [ { "type": "variable_if", "name": "vim_sticky", "value": 0 } ] }
            ]
        },
        {
            "description": "3. Escape (Panic Button: Only active if Mode is ON)",
            "type": "basic",
            "conditions": [ { "type": "variable_if", "name": "vim_mode", "value": 1 } ],
            "from": { "key_code": "escape", "modifiers": { "optional": ["any"] } },
            "to": [
                { "shell_command": "osascript -e 'display notification \"üèÅ Mode Off\" with title \"Vim Mode\"'" },
                { "key_code": "right_arrow", "conditions": [ { "type": "variable_if", "name": "visual_mode", "value": 1 } ] },
                { "key_code": "escape" },
                { "set_variable": { "name": "vim_sticky", "value": 0 } },
                { "set_variable": { "name": "vim_mode", "value": 0 } },
                { "set_variable": { "name": "delete_pending", "value": 0 } },
                { "set_variable": { "name": "visual_mode", "value": 0 } }
            ]
        },
        {
            "description": "========== Command Shortcuts (Global Fast Scroll) ==========",
            "type": "basic"
        },
        {
            "description": "Cmd+Enter -> End + Enter",
            "type": "basic",
            "from": { "key_code": "return_or_enter", "modifiers": { "mandatory": ["left_command"] } },
            "to": [ { "key_code": "right_arrow", "modifiers": ["left_command"] }, { "key_code": "return_or_enter" } ]
        },
        {
            "description": "Cmd+I (Fast Up - Paragraph)",
            "type": "basic",
            "from": { "key_code": "i", "modifiers": { "mandatory": ["left_command"] } },
            "to": [ { "key_code": "up_arrow", "modifiers": ["left_option"] } ]
